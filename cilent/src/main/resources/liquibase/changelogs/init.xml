<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="20210127-01" author="lqq">
        <createTable tableName="test">
            <column name="id" type="bigint(64)" encoding="utf8" >
                <constraints primaryKey="true"  />
            </column>
            <column name="test_name" type="varchar(50)" encoding="utf8" remarks="项目名字"/>
            <column name="test_addr" type="varchar(50)" encoding="utf8" remarks="项目地址"/>
        </createTable>
    </changeSet>
</databaseChangeLog>